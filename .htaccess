<Files .htaccess>
Order Deny,Allow
Deny from all
</Files>

<ifDefine Free>
 PHP56 1
</ifDefine>

# Cache le PHPSESSID de l'url
SetEnv SESSION_USE_TRANS_SID 0

# Bloque l'accès à la liste des fichiers
Options -Indexes -MultiViews

# Bloque l'accès au fichier de données
<Files data.json>
Order allow,deny
Deny from all
</Files>
